{
  "hosting": {
    "public": "dist/frontend",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/api1{,/**}",
        "run": {
          "serviceId": "api1",
          "region": "asia-northeast1"
        }
      }
    ]
  }
}
